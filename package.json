{
  "name": "treehouse-project-8",
  "version": "1.0.0",
  "description": "Using gulp to build a front end website",
  "main": "gulpfile.js",
  "scripts": {
    "build": "gulp build",
    "start": "gulp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ermaxnet/treehouse-project-8.git"
  },
  "author": "Max Eremin",
  "license": "MIT",
  "paths": {
    "dist": {
      "js": "./dist/scripts/",
      "css": "./dist/styles/",
      "images": "./dist/content/images/",
      "icons": "./dist/content/icons/",
      "template": "./dist/"
    },
    "src": {
      "js": "./js/",
      "scss": "./sass/",
      "images": "./images/",
      "template": "./index.html"
    },
    "build": {
      "css": "./build/css/"
    }
  },
  "globs": {
    "distJs": [
      "./node_modules/jquery/dist/jquery.min.js",
      "./js/**/*.js"
    ],
    "distCss": [
      "./build/css/*.css"
    ],
    "distIcons": [
      "./icons/*.{eot,svg,ttf,woff}"
    ]
  },
  "vars": {
    "scssName": "global.scss"
  },
  "devDependencies": {
    "del": "^3.0.0",
    "fancy-log": "^1.3.2",
    "git-rev-sync": "^1.11.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-cached": "^1.1.1",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-header": "^2.0.5",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.1.0",
    "gulp-newer": "^1.4.0",
    "gulp-plumber": "^1.2.0",
    "gulp-print": "^5.0.0",
    "gulp-replace": "^0.6.1",
    "gulp-sass": "^3.2.1",
    "gulp-server-livereload": "^1.9.2",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.0",
    "gulp-useref": "^3.1.5",
    "moment": "^2.22.0",
    "run-sequence": "^2.2.1"
  },
  "dependencies": {
    "jquery": "^3.3.1"
  }
}
